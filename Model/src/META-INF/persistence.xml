    <properties>
      <property name="eclipselink.target-server" value="WebLogic_10"/>
      <property name="javax.persistence.jtaDataSource" value="java:/app/jdbc/jdbc/INF5180-10DS"/>
    </properties>
  </persistence-unit>
</persistence>
